import java.util.*;

public class Second {
    public static void main(String args[]) {
        Scanner scanner = new Scanner(System.in);
        Integer count = scanner.nextInt();
        int result = 0;
        int temp =0;
        for(int i=0;i<count;i++){
            Integer input = scanner.nextInt();
            temp = temp^input;
            if(input==0||temp==0){
                result++;
                temp = 0;
            }
        }
        System.out.println(result);
    }
}
