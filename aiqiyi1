
import java.util.*;

public class Second {
    public static void main(String args[]) {
        Scanner scanner = new Scanner(System.in);
        String input = scanner.next();
        char[] chars =input.toCharArray();
        int rCount =0;
        int result = 0;
        for (char aChar : chars) {
            if(aChar=='R')
                rCount++;
        }
        for(int i =0;i<rCount;i++){
            if(chars[i]=='G')
                result++;
        }
        System.out.println(result*2);
    }
}
